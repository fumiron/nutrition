<% if user_signed_in? && current_user.id == food.user_id %>
<tbody>
  <tr>
    <td class="bottom_name"><%= link_to food.name, food_path(food.id), class: :food__name %></td>
    <td class="bottom"><%= food.calorie %></td>
    <td class="bottom"><%= food.protein %></td>
    <td class="bottom"><%= food.fat %></td>
    <td class="bottom"><%= food.carbohydrate %></td>
    <td class="bottom"><%= food.vitamin_a %></td>
    <td class="bottom"><%= food.vitamin_b1 %></td>
    <td class="bottom"><%= food.vitamin_b2 %></td>
    <td class="bottom"><%= food.vitamin_c %></td>
    <td class="bottom"><%= food.sodium %></td>
    <td class="bottom"><%= food.calcium %></td>
    <td class="bottom"><%= food.iron %></td>
    <td class="bottom"><%= food.zinc %></td>
    <td class="bottom"><%= food.salt %></td>
  </tr>
</tbody>
<% end %>